  buttonState_1 = digitalRead(button_1);
  buttonState_2 = digitalRead(button_2);
  buttonState_3 = digitalRead(button_3);
  buttonState_4 = digitalRead(button_4);

  
  if (buttonState_1  == HIGH & (millis()-startTime_1) > interval_1) {  //Contador para cambios de pantallas, permita cambiar cada que recora el tiempo de interval_1
    button_cont_1++;
